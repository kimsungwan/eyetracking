    <style>
        /* Custom overrides for Uvolution */
        .visual {
            background: #1a1a1a;
        }

        .visual h1 {
            color: #fff;
        }

        .visual .tagline {
            color: #a3a3a3;
        }

        #deepgaze-widget-section {
            padding: 60px 0;
            background: #fff;
        }

        /* SINGLE-LINE HEADER LAYOUT */
        #header .container {
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        #header .logo {
            flex-shrink: 0;
        }

        #header .logo a {
            display: flex;
            align-items: center;
        }

        #header #nav {
            flex-grow: 1;
            display: flex;
            justify-content: flex-end;
            position: static;
            width: auto;
        }

        #header #nav .opener-holder {
            display: none;
        }

        #header #nav .nav-drop {
            display: flex !important;
            align-items: center;
            position: static;
            background: none;
            padding: 0;
            width: auto;
        }

        #header #nav .nav-drop>ul {
            display: flex !important;
            flex-direction: row;
            margin: 0;
            padding: 0;
            list-style: none;
            align-items: center;
        }

        #header #nav .nav-drop>ul li {
            margin: 0 10px;
        }

        #header #nav .drop-holder {
            display: none;
        }

        /* Mobile responsive */
        @media (max-width: 1023px) {
            #header .container {
                display: block;
            }

            #header #nav .opener-holder {
                display: block;
            }

            #header #nav .nav-drop {
                display: none !important;
            }
        }
    </style>
</head>

